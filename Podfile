project 'HotMess.xcodeproj'

# Uncomment the next line to define a global platform for your project

target 'HotMess' do
  platform :ios, '10.0'
  
  
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  inhibit_all_warnings!

  # Pods for HotMess
  pod 'Alamofire'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Locksmith'
  pod 'Starscream'
  pod 'GeoJSON'
  pod 'SwiftyJSON'
  pod 'Kingfisher'
  pod 'Firebase/AdMob'
  pod 'Mixpanel'
  pod 'Fabric'
  pod 'Crashlytics'
  
  target 'HotMessTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HotMessUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
