project 'HotMess.xcodeproj'

# Uncomment the next line to define a global platform for your project

target 'HotMess' do
  platform :ios, '11.0'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HotMess
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Firebase/Core'
  pod 'Firebase/AdMob'
  pod 'Firebase/Crash'
  pod 'Alamofire'
  pod 'Locksmith'
  pod 'Starscream'
  pod 'GeoJSON', git: 'https://github.com/rickmark/GeoJSON.swift.git'
  pod 'SwiftyJSON'
  pod 'Kingfisher'
  pod 'Mixpanel'

  target 'HotMessTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HotMessUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
